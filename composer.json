{
  "name": "rozehnal/environment-parameters",
  "description": "Composer script building your detached parameters for deploying the app. Built above the great incenteev/composer-parameter-handler library.",
  "keywords": ["parameters management", "configuration", "environment management"],
  "homepage": "https://github.com/rozehnal/environment-parameters",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Rozehnal",
      "email": "rozehnal@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "incenteev/composer-parameter-handler": "~2.0",
    "symfony/yaml": "~2.3|~3.0",
    "symfony/filesystem": "~2.8|~3.2"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Paro\\EnvironmentParameters\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}